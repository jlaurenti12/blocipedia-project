<div class='header'>
  <h1> Wikis </h1>
  <%= link_to "Create a New Wiki", new_wiki_path, class: 'btn btn-success' %>
</div>

<div class="row">
      <% @wikis.each do |wiki| %>
        <div class="col-sm-6 col-md-4">
          <div class="thumbnail">
            <div class="caption">
              <h3> <%= wiki.title %> </h3>
              <h6> <%= wiki.body %> </h6>
              <%= link_to "View Full Wiki", wiki %>
            </div>
          </div>
        </div>
          <% end %>
      </div>
